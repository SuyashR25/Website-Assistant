{
  "manifest_version": 3,
  "name": "Website Assistant",
  "version": "1.0",
  "description": "AI Assistant for asking questions about the current webpage.",
  "permissions": [
    "activeTab"
  ],
  "host_permissions": [
    "http://127.0.0.1:5000/*"
  ],
  "action": {
    "default_popup": "index.html",
    "default_title": "Chat with this page"
  }
}